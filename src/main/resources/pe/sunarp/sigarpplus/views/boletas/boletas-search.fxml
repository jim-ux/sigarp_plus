<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="575.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pe.sunarp.sigarpplus.controllers.boletas.BoletasSearchController">
   <children>
      <HBox alignment="CENTER_LEFT" layoutX="40.0" layoutY="23.0" prefHeight="27.0" prefWidth="800.0" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Seleccione año:" />
            <ComboBox fx:id="cbxYear" onAction="#cbxYearOnClick" prefWidth="150.0" promptText="SELECCIONE" />
            <Label text="Seleccione mes:" />
            <ComboBox fx:id="cbxMonth" disable="true" prefWidth="150.0" promptText="SELECCIONE" />
            <Button mnemonicParsing="false" onAction="#btnSearchOnClick" prefHeight="25.0" prefWidth="94.0" text="Buscar" />
         </children>
      </HBox>
      <TableView fx:id="tbvTrabs" layoutX="40.0" layoutY="40.0" prefHeight="474.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="45.0">
        <columns>
          <TableColumn fx:id="tbcCodTrab" prefWidth="91.0" text="Código" />
          <TableColumn fx:id="tbcApPaterno" prefWidth="121.0" text="Apellido Paterno" />
            <TableColumn fx:id="tbcApMaterno" prefWidth="198.0" text="Apellido Materno" />
            <TableColumn fx:id="tbcNombres" prefWidth="75.0" text="Nombres" />
            <TableColumn fx:id="tbcDocumento" prefWidth="75.0" text="Documento" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</AnchorPane>
